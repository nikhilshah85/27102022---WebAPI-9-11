


<script>
    function getProductsFromAPI()
    {

        fetch('https://localhost:7202/api/Products/plist').then( response => response.json())
                                                          .then( result => {
                                                          console.log(result);
                                                          alert('success');



                                                          for(var i=0;i<result.length;i++)
                                                          {
                                                          $("table").append("<tr>" + 
                                                          "<td>" + result[i].pId+ "</td>" +
                                                          "<td>" + result[i].pName+ "</td>" +
                                                          "<td>" + result[i].pCategory+ "</td>" +
                                                          "<td>" + result[i].pPrice+ "</td>" +
                                                          "<td>" + result[i].pIsInStock+ "</td>" +
                                                          
                                                          
                                                          + "</tr>")
                                                          }
                                                         })
                                                          .catch(err=>{
                                                              console.log(err);
                                                              alert('Errorrrr');
                                                          })
    }
</script>


<button class="btn btn-primary" onClick="getProductsFromAPI()"> Display Products  </button>
<table class="table table-bordered"></table>